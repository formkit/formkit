{
  "name": "@formkit/i18n",
  "version": "1.6.2",
  "type": "module",
  "description": "Internationalization layer for FormKit.",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    "./locales/*": {
      "import": {
        "types": "./dist/locales/*.d.ts",
        "development": "./dist/locales/*.dev.mjs",
        "default": "./dist/locales/*.mjs"
      },
      "require": {
        "types": "./dist/locales/*.d.cts",
        "default": "./dist/locales/*.cjs"
      }
    },
    "./i18n": {
      "import": {
        "types": "./dist/i18n.d.ts",
        "development": "./dist/i18n.dev.mjs",
        "default": "./dist/i18n.mjs"
      },
      "require": {
        "types": "./dist/i18n.d.cts",
        "default": "./dist/i18n.cjs"
      }
    },
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "development": "./dist/index.dev.mjs",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": "./dist/*"
  },
  "keywords": [
    "vue",
    "forms",
    "inputs",
    "validation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/formkit/formkit.git",
    "directory": "packages/i18n"
  },
  "contributors": [
    "Justin Schroeder <justin@formkit.com>",
    "Andrew Boyd <andrew@formkit.com>"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "@formkit/core": "1.6.2",
    "@formkit/utils": "1.6.2",
    "@formkit/validation": "1.6.2"
  }
}
