{
  "name": "@formkit/addons",
  "type": "module",
  "version": "1.6.2",
  "description": "Optional features you can add to your FormKit inputs.",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "development": "./dist/index.dev.mjs",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./css/multistep": "./dist/css/multistep.css",
    "./css/floatingLabels": "./dist/css/floatingLabels.css",
    "./*": "./dist/*"
  },
  "keywords": [
    "vue",
    "forms",
    "inputs",
    "validation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/formkit/formkit.git",
    "directory": "packages/addons"
  },
  "contributors": [
    "Justin Schroeder <justin@formkit.com>",
    "Andrew Boyd <andrew@formkit.com>"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "@formkit/auto-animate": ">=0.8.2",
    "@formkit/core": "1.6.2",
    "@formkit/inputs": "1.6.2",
    "@formkit/utils": "1.6.2"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@formkit/vue": "workspace:^"
  }
}
